<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='kune_color_picker'>
  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='net.auroris.ColorPicker.client'/>
  <!-- Other module inherits -->

  <!-- Specify the app entry point class. -->

  <entry-point class='cc.kune.colorpicker.client.KuneColorPickerEntryPoint'/>

  <!-- Specify the paths for translatable code -->
  <source path='client'/>
  <public path="public"/>

  <!-- For make CSS obfuscation more pretty -->
  <set-configuration-property name="CssResource.style"
    value="pretty"/>

  <!-- <stylesheet src="somefile.css"/> -->

</module>
