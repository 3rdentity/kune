<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:k="urn:import:cc.kune.common.client.ui.dialogs"
  >
  <ui:style>
  
  </ui:style>
  <b:Modal ui:field="modal" title="sometitle" closable="true" fade="true"
    dataBackdrop="STATIC" dataKeyboard="true" b:id="myModal">
    <k:CustomModalHeader ui:field="modalHeader" >
    </k:CustomModalHeader>
    <b:ModalBody>
      <g:FlowPanel>
        <g:FlowPanel ui:field="flow">
          <g:FlowPanel ui:field="vp"/>
          <g:FlowPanel ui:field="bottomPanel"/> <!-- width 100% originally -->
        </g:FlowPanel>
      </g:FlowPanel>
    </b:ModalBody>
    <b:ModalFooter>
      <g:FlowPanel ui:field="btnPanel">
        <b:Button ui:field="firstBtn"></b:Button>
        <b:Button ui:field="secondBtn" dataDismiss="MODAL"></b:Button>
      </g:FlowPanel>
    </b:ModalFooter>
  </b:Modal>

</ui:UiBinder>