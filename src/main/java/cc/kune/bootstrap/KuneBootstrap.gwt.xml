<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
<module>
  <inherits name="com.google.gwt.user.User" />
  <inherits name="org.gwtbootstrap3.GwtBootstrap3" />
  <inherits name="org.gwtbootstrap3.extras.growl.Growl"/>
  <inherits name="org.gwtbootstrap3.extras.bootbox.Bootbox"/>
  <inherits name="cc.kune.bootstrap.client.smartmenus.SmartMenus"/>
  
  <source path="client" />
  <source path="resource/js"/>
  
  <public path='resource'>
    <include name='css/*.css' />
    <include name='js/*.js' />
  </public>

  <stylesheet src='css/theme.css' />

</module>
