<module>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.i18n.I18N" />
	<!-- <inherits name="org.mcarthur.sandy.gwt.login.Login"/> -->
	<inherits name="org.gwm.GwtWindowManager" />
	<!-- <inherits name='org.gwtwidgets.WidgetLibrary'/> -->
	<entry-point class="org.ourproject.kune.client.Main" />
	<extend-property name="locale" values="es" />
	<extend-property name="locale" values="ar" />
	<servlet path="/XmppService"
		class="org.ourproject.kune.server.servlet.XmppServiceServlet" />
	<servlet path="/KuneDocumentService"
		class="org.ourproject.kune.server.servlet.KuneDocumentServiceServlet" />
	<!--http://code.google.com/p/google-web-toolkit/issues/detail?id=42 -->
	<set-property name="user.agent" value="gecko" />
</module>
